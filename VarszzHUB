-- Load Library KHUSUS ANDROID (pake yang ringan)
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))() or loadstring(game:HttpGet("https://pastebin.com/raw/gyjn3WrA"))()

-- Tunggu bentar biar library ke-load
task.wait(2)

-- Variables
local player = game.Players.LocalPlayer
local autoFarm = false
local autoMoney = false
local teleportToArea = false
local godMode = false
local speedBoost = false
local jumpBoost = false
local antiTsunami = false

local targetRarities = {
    Secret = true,
    Celestial = true,
    Divine = true
}

-- =============================================
-- üîë KEY SYSTEM MANUAL (PALING SEDERHANA)
-- =============================================
local key = "varszzscript"
local keyGui = Instance.new("ScreenGui")
local keyFrame = Instance.new("Frame")
local keyLabel = Instance.new("TextLabel")
local keyBox = Instance.new("TextBox")
local keyButton = Instance.new("TextButton")

keyGui.Parent = player:WaitForChild("PlayerGui")
keyFrame.Size = UDim2.new(0, 300, 0, 200)
keyFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
keyFrame.BackgroundColor3 = Color3.new(0.1, 0.1, 0.15)
keyFrame.BackgroundTransparency = 0.1
keyFrame.Active = true
keyFrame.Draggable = true
keyFrame.Parent = keyGui

-- Biar keren dikit
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 10)
corner.Parent = keyFrame

keyLabel.Size = UDim2.new(1, 0, 0, 40)
keyLabel.Position = UDim2.new(0, 0, 0, 10)
keyLabel.Text = "üîê VARSZZHUB V7"
keyLabel.TextColor3 = Color3.new(1, 1, 0)
keyLabel.TextScaled = true
keyLabel.BackgroundTransparency = 1
keyLabel.Font = Enum.Font.GothamBold
keyLabel.Parent = keyFrame

local subLabel = Instance.new("TextLabel")
subLabel.Size = UDim2.new(1, 0, 0, 30)
subLabel.Position = UDim2.new(0, 0, 0, 55)
subLabel.Text = "Masukkan Key"
subLabel.TextColor3 = Color3.new(1, 1, 1)
subLabel.TextScaled = true
subLabel.BackgroundTransparency = 1
subLabel.Font = Enum.Font.Gotham
subLabel.Parent = keyFrame

keyBox.Size = UDim2.new(0.8, 0, 0, 40)
keyBox.Position = UDim2.new(0.1, 0, 0, 95)
keyBox.PlaceholderText = "varszzscript"
keyBox.Text = ""
keyBox.TextScaled = true
keyBox.BackgroundColor3 = Color3.new(0.2, 0.2, 0.25)
keyBox.TextColor3 = Color3.new(1, 1, 1)
keyBox.Font = Enum.Font.Gotham
keyBox.ClearTextOnFocus = false
keyBox.Parent = keyFrame

local boxCorner = Instance.new("UICorner")
boxCorner.CornerRadius = UDim.new(0, 5)
boxCorner.Parent = keyBox

keyButton.Size = UDim2.new(0.5, 0, 0, 40)
keyButton.Position = UDim2.new(0.25, 0, 0, 145)
keyButton.Text = "MASUK"
keyButton.TextScaled = true
keyButton.BackgroundColor3 = Color3.new(0, 0.5, 1)
keyButton.TextColor3 = Color3.new(1, 1, 1)
keyButton.Font = Enum.Font.GothamBold
keyButton.Parent = keyFrame

local btnCorner = Instance.new("UICorner")
btnCorner.CornerRadius = UDim.new(0, 5)
btnCorner.Parent = keyButton

-- Fungsi key
keyButton.MouseButton1Click:Connect(function()
    if keyBox.Text == key then
        keyGui:Destroy()
        task.wait(0.5)
        createMainUI()
        -- Notifikasi sambutan
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "üëã HALLO",
            Text = "Selamat datang di VARSZZHUB V7 üî•",
            Duration = 3
        })
    else
        keyBox.Text = ""
        keyBox.PlaceholderText = "‚ùå SALAH!"
    end
end)

-- =============================================
-- üéõÔ∏è FUNGSI UTAMA
-- =============================================
function createMainUI()
    -- Pake UI Sederhana tapi Work di Android
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "VarszzHub"
    screenGui.Parent = player:WaitForChild("PlayerGui")
    
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 350, 0, 450)
    mainFrame.Position = UDim2.new(0.5, -175, 0.5, -225)
    mainFrame.BackgroundColor3 = Color3.new(0.1, 0.1, 0.15)
    mainFrame.Active = true
    mainFrame.Draggable = true
    mainFrame.Parent = screenGui
    
    local mainCorner = Instance.new("UICorner")
    mainCorner.CornerRadius = UDim.new(0, 10)
    mainCorner.Parent = mainFrame
    
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 40)
    title.Text = "üåü VARSZZHUB V7 - ANDROID"
    title.TextColor3 = Color3.new(1, 1, 0)
    title.TextScaled = true
    title.BackgroundColor3 = Color3.new(0.2, 0.2, 0.25)
    title.Font = Enum.Font.GothamBold
    title.Parent = mainFrame
    
    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 40, 0, 40)
    closeBtn.Position = UDim2.new(1, -40, 0, 0)
    closeBtn.Text = "X"
    closeBtn.TextColor3 = Color3.new(1, 0, 0)
    closeBtn.BackgroundTransparency = 1
    closeBtn.Font = Enum.Font.GothamBold
    closeBtn.Parent = mainFrame
    closeBtn.MouseButton1Click:Connect(function() screenGui:Destroy() end)
    
    local yPos = 50
    
    -- Fungsi bikin toggle sederhana
    local function createToggle(name, default, callback)
        local toggleFrame = Instance.new("Frame")
        toggleFrame.Size = UDim2.new(0.9, 0, 0, 40)
        toggleFrame.Position = UDim2.new(0.05, 0, 0, yPos)
        toggleFrame.BackgroundColor3 = Color3.new(0.15, 0.15, 0.2)
        toggleFrame.Parent = mainFrame
        
        local toggleCorner = Instance.new("UICorner")
        toggleCorner.CornerRadius = UDim.new(0, 5)
        toggleCorner.Parent = toggleFrame
        
        local toggleLabel = Instance.new("TextLabel")
        toggleLabel.Size = UDim2.new(0.7, 0, 1, 0)
        toggleLabel.Text = name
        toggleLabel.TextColor3 = Color3.new(1, 1, 1)
        toggleLabel.TextScaled = true
        toggleLabel.BackgroundTransparency = 1
        toggleLabel.Font = Enum.Font.Gotham
        toggleLabel.Parent = toggleFrame
        
        local toggleBtn = Instance.new("TextButton")
        toggleBtn.Size = UDim2.new(0.2, 0, 0.8, 0)
        toggleBtn.Position = UDim2.new(0.75, 0, 0.1, 0)
        toggleBtn.Text = default and "ON" or "OFF"
        toggleBtn.TextScaled = true
        toggleBtn.BackgroundColor3 = default and Color3.new(0, 0.8, 0) or Color3.new(0.5, 0, 0)
        toggleBtn.Font = Enum.Font.GothamBold
        toggleBtn.Parent = toggleFrame
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 5)
        btnCorner.Parent = toggleBtn
        
        local state = default
        toggleBtn.MouseButton1Click:Connect(function()
            state = not state
            toggleBtn.Text = state and "ON" or "OFF"
            toggleBtn.BackgroundColor3 = state and Color3.new(0, 0.8, 0) or Color3.new(0.5, 0, 0)
            callback(state)
        end)
        
        yPos = yPos + 45
        return toggleFrame
    end
    
    -- Notifikasi
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "‚úÖ VARSZZHUB V7 SIAP",
        Text = "Aktifkan fitur di bawah!",
        Duration = 3
    })
    
    -- =============================================
    -- FITUR-FITUR
    -- =============================================
    
    -- God Mode
    createToggle("üõ°Ô∏è God Mode", false, function(val)
        godMode = val
        if val then
            spawn(function()
                while godMode do
                    task.wait(0.1)
                    pcall(function()
                        if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                            player.Character.Humanoid.Health = math.huge
                        end
                    end)
                end
            end)
        end
    end)
    
    -- Speed Boost
    createToggle("‚ö° Speed Boost", false, function(val)
        speedBoost = val
        if val then
            spawn(function()
                while speedBoost do
                    task.wait(0.1)
                    pcall(function()
                        if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                            player.Character.Humanoid.WalkSpeed = 250
                        end
                    end)
                end
            end)
        else
            pcall(function()
                if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                    player.Character.Humanoid.WalkSpeed = 16
                end
            end)
        end
    end)
    
    -- Auto Farm
    createToggle("üåæ Auto Farm", false, function(val)
        autoFarm = val
        if val then
            spawn(function()
                while autoFarm do
                    task.wait(0.3)
                    pcall(function()
                        for _, obj in pairs(workspace:GetDescendants()) do
                            if obj:IsA("Part") and obj.Name:lower():find("brainrot") and player.Character then
                                local root = player.Character:FindFirstChild("HumanoidRootPart")
                                if root then
                                    root.CFrame = CFrame.new(obj.Position + Vector3.new(0, 2, 0))
                                    task.wait(0.1)
                                    firetouchinterest(root, obj, 0)
                                    task.wait(0.05)
                                    firetouchinterest(root, obj, 1)
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end)
    
    -- Auto Money
    createToggle("üí∞ Auto Money", false, function(val)
        autoMoney = val
        if val then
            spawn(function()
                while autoMoney do
                    task.wait(2)
                    pcall(function()
                        local claim = workspace:FindFirstChild("ClaimMoney") or player.PlayerGui:FindFirstChild("ClaimButton")
                        if claim and claim:IsA("TextButton") then
                            claim:Click()
                        end
                    end)
                end
            end)
        end
    end)
    
    -- Anti Tsunami
    createToggle("üåä Anti Tsunami", false, function(val)
        antiTsunami = val
        if val then
            spawn(function()
                while antiTsunami do
                    task.wait(0.5)
                    pcall(function()
                        local tsunami = workspace:FindFirstChild("Tsunami")
                        if tsunami and player.Character then
                            local dist = (player.Character.HumanoidRootPart.Position - tsunami.Position).Magnitude
                            if dist < 50 then
                                local safe = workspace:FindFirstChild("SafeZone") or workspace:FindFirstChild("Spawn")
                                if safe then
                                    player.Character.HumanoidRootPart.CFrame = safe.CFrame
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end)
    
    -- Credit
    local credit = Instance.new("TextLabel")
    credit.Size = UDim2.new(0.9, 0, 0, 30)
    credit.Position = UDim2.new(0.05, 0, 0, yPos + 10)
    credit.Text = "by Alvaro | Key: varszzscript"
    credit.TextColor3 = Color3.new(0.5, 0.5, 0.5)
    credit.TextScaled = true
    credit.BackgroundTransparency = 1
    credit.Font = Enum.Font.Gotham
    credit.Parent = mainFrame
end