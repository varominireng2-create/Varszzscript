-- FUNGSI KEY
local expectedKey = "varszzscript"

submitBtn.MouseButton1Click:Connect(function()
    local inputKey = keyInput.Text:lower():gsub("%s+", "")
    
    if inputKey == expectedKey then
        -- Animasi sukses
        submitBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
        submitBtn.Text = "‚úì BERHASIL"
        statusLabel.Text = "‚úÖ Key benar! Loading..."
        statusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        
        task.wait(1)
        keyGui:Destroy()
        
        -- Notifikasi
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "‚úÖ SELAMAT DATANG",
            Text = "VARSZZHUBüî• V7 AKTIF",
            Duration = 3
        })
        
        -- Panggil GUI utama
        createMainUI()
    else
        -- Animasi gagal
        submitBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
        submitBtn.Text = "‚úó SALAH"
        statusLabel.Text = "‚ùå Key salah! Coba lagi"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        keyInput.Text = ""
        keyInput.PlaceholderText = "Coba lagi..."
        
        task.wait(1)
        submitBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
        submitBtn.Text = "MASUK"
    end
end)

closeBtn.MouseButton1Click:Connect(function()
    keyGui:Destroy()
end)

-- =============================================
-- GUI UTAMA (ANDROID TOUCH FRIENDLY)
-- =============================================
function createMainUI()
    local mainGui = Instance.new("ScreenGui")
    mainGui.Name = "VarszzHub"
    mainGui.Parent = player.PlayerGui
    mainGui.ResetOnSpawn = false
    mainGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    -- Simpan referensi global untuk minimize
    mainGuiRef = mainGui
    
    -- Variables
    local autoFarm = false
    local autoMoney = false
    local godMode = false
    local speedBoost = false
    local jumpBoost = false
    local antiTsunami = false
    
    -- Main container (lebih kecil biar muat di layar HP)
    local main = Instance.new("Frame")
    main.Size = UDim2.new(0, 320, 0, 500)
    main.Position = UDim2.new(0.5, -160, 0.5, -250)
    main.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    main.BackgroundTransparency = 0.05
    main.Active = true
    main.Draggable = false
    main.Parent = mainGui
    
    local mainCorner = Instance.new("UICorner")
    mainCorner.CornerRadius = UDim.new(0, 24)
    mainCorner.Parent = main
    
    addShadow(main, 30, 0.5)
    
    -- Header dengan gradient
    local header = Instance.new("Frame")
    header.Size = UDim2.new(1, 0, 0, 80)
    header.BackgroundColor3 = Color3.fromRGB(30, 30, 45)
    header.Parent = main
    
    local headerCorner = Instance.new("UICorner")
    headerCorner.CornerRadius = UDim.new(0, 24)
    headerCorner.Parent = header
    
    local headerGradient = Instance.new("UIGradient")
    headerGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(50, 50, 80)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(30, 30, 45))
    })
    headerGradient.Rotation = 90
    headerGradient.Parent = header
    
    -- Title in header
    local mainTitle = Instance.new("TextLabel")
    mainTitle.Size = UDim2.new(1, -50, 1, 0)
    mainTitle.Position = UDim2.new(0, 20, 0, 0)
    mainTitle.BackgroundTransparency = 1
    mainTitle.Text = "VARSZZHUBüî• V7"
    mainTitle.TextColor3 = Color3.fromRGB(255, 215, 0)
    mainTitle.TextSize = 24
    mainTitle.TextXAlignment = Enum.TextXAlignment.Left
    mainTitle.Font = Enum.Font.GothamBold
    mainTitle.Parent = header
    
    -- Subtitle
    local mainSub = Instance.new("TextLabel")
    mainSub.Size = UDim2.new(1, -50, 0, 20)
    mainSub.Position = UDim2.new(0, 20, 0, 45)
    mainSub.BackgroundTransparency = 1
    mainSub.Text = "Android Edition ‚Ä¢ varszzscript"
    mainSub.TextColor3 = Color3.fromRGB(150, 150, 200)
    mainSub.TextSize = 12
    mainSub.TextXAlignment = Enum.TextXAlignment.Left
    mainSub.Font = Enum.Font.Gotham
    mainSub.Parent = header
    
    -- Close button di header (SEKARANG JADI MINIMIZE)
    local headerClose = Instance.new("ImageButton")
    headerClose.Size = UDim2.new(0, 40, 0, 40)
    headerClose.Position = UDim2.new(1, -45, 0, 20)
    headerClose.BackgroundTransparency = 1
    headerClose.Image = "rbxassetid://6031090990"
    headerClose.ImageColor3 = Color3.fromRGB(255, 100, 100)
    headerClose.Parent = header
    
    -- MINIMIZE KE ICON (BUKAN DESTROY)
    headerClose.MouseButton1Click:Connect(function()
        minimizeToIcon()
    end)
    
    -- ScrollFrame (biar fitur banyak bisa discroll)
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, -20, 1, -100)
    scroll.Position = UDim2.new(0, 10, 0, 90)
    scroll.BackgroundTransparency = 1
    scroll.ScrollBarThickness = 4
    scroll.ScrollBarImageColor3 = Color3.fromRGB(100, 100, 255)
    scroll.CanvasSize = UDim2.new(0, 0, 0, 0)
    scroll.Parent = main
    
    local yPos = 0
    
    -- FUNGSI MEMBUAT CARD FITUR (BESAR, MUDAH DISENTUH)
    local function createFeatureCard(icon, title, desc, defaultColor, callback)
        local card = Instance.new("Frame")
        card.Size = UDim2.new(1, -10, 0, 80)
        card.Position = UDim2.new(0, 5, 0, yPos)
        card.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        card.BackgroundTransparency = 0.2
        card.Parent = scroll
        
        local cardCorner = Instance.new("UICorner")
        cardCorner.CornerRadius = UDim.new(0, 16)
        cardCorner.Parent = card
        
        -- Icon
        local iconImg = Instance.new("ImageLabel")
        iconImg.Size = UDim2.new(0, 40, 0, 40)
        iconImg.Position = UDim2.new(0, 15, 0.5, -20)
        iconImg.BackgroundTransparency = 1
        iconImg.Image = icon
        iconImg.ImageColor3 = defaultColor or Color3.fromRGB(255, 255, 255)
        iconImg.Parent = card
        
        -- Title
        local titleLbl = Instance.new("TextLabel")
        titleLbl.Size = UDim2.new(1, -130, 0, 25)
        titleLbl.Position = UDim2.new(0, 65, 0, 15)
        titleLbl.BackgroundTransparency = 1
        titleLbl.Text = title
        titleLbl.TextColor3 = Color3.fromRGB(255, 255, 255)
        titleLbl.TextSize = 18
        titleLbl.TextXAlignment = Enum.TextXAlignment.Left
        titleLbl.Font = Enum.Font.GothamBold
        titleLbl.Parent = card
        
        -- Description
        local descLbl = Instance.new("TextLabel")
        descLbl.Size = UDim2.new(1, -130, 0, 20)
        descLbl.Position = UDim2.new(0, 65, 0, 40)
        descLbl.BackgroundTransparency = 1
        descLbl.Text = desc
        descLbl.TextColor3 = Color3.fromRGB(150, 150, 150)
        descLbl.TextSize = 12
        descLbl.TextXAlignment = Enum.TextXAlignment.Left
        descLbl.Font = Enum.Font.Gotham
        descLbl.Parent = card
        
        -- Toggle Switch (besar biar gampang disentuh)
        local toggleBtn = Instance.new("TextButton")
        toggleBtn.Size = UDim2.new(0, 60, 0, 40)
        toggleBtn.Position = UDim2.new(1, -70, 0.5, -20)
        toggleBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 100)
        toggleBtn.Text = "OFF"
        toggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
        toggleBtn.TextSize = 14
        toggleBtn.Font = Enum.Font.GothamBold
        toggleBtn.Parent = card
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 20)
        btnCorner.Parent = toggleBtn
        
        local active = false
        local colorOn = defaultColor or Color3.fromRGB(0, 150, 255)
        
        toggleBtn.MouseButton1Click:Connect(function()
            active = not active
            toggleBtn.Text = active and "ON" or "OFF"
            toggleBtn.BackgroundColor3 = active and colorOn or Color3.fromRGB(80, 80, 100)
            callback(active)
        end)
        
        yPos = yPos + 85
        scroll.CanvasSize = UDim2.new(0, 0, 0, yPos + 10)
        
        return card
    end
    
    -- =============================================
    -- DAFTAR FITUR (PAKE ICON KEREN)
    -- =============================================
    
    -- God Mode
    createFeatureCard(
        "rbxassetid://6031068443",  -- Icon shield/lock
        "God Mode", 
        "Health tak terbatas",
        Color3.fromRGB(255, 100, 100),
        function(val) godMode = val 
            if val then
                spawn(function()
                    while godMode do
                        task.wait(0.1)
                        pcall(function()
                            if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                                player.Character.Humanoid.Health = math.huge
                            end
                        end)
                    end
                end)
            end
        end
    )
    
    -- Speed Boost
    createFeatureCard(
        "rbxassetid://6031090990",  -- Icon speed
        "Speed Boost", 
        "Jalan super cepat (250)",
        Color3.fromRGB(100, 255, 100),
        function(val) speedBoost = val 
            if val then
                spawn(function()
                    while speedBoost do
                        task.wait(0.1)
                        pcall(function()
                            if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                                player.Character.Humanoid.WalkSpeed = 250
                            end
                        end)
                    end
                end)
            else
                pcall(function()
                    if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                        player.Character.Humanoid.WalkSpeed = 16
                    end
                end)
            end
        end
    )
    
    -- Jump Boost
    createFeatureCard(
        "rbxassetid://6031071084",  -- Icon jump
        "Jump Boost", 
        "Loncat lebih tinggi",
        Color3.fromRGB(100, 255, 255),
        function(val) jumpBoost = val 
            if val then
                spawn(function()
                    while jumpBoost do
                        task.wait(0.1)
                        pcall(function()
                            if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                                player.Character.Humanoid.JumpPower = 100
                            end
                        end)
                    end
                end)
            else
                pcall(function()
                    if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                        player.Character.Humanoid.JumpPower = 50
                    end
                end)
            end
        end
    )
    
    -- Auto Farm
    createFeatureCard(
        "rbxassetid://6031088344",  -- Icon farm
        "Auto Farm", 
        "Farming brainrot otomatis",
        Color3.fromRGB(255, 200, 100),
        function(val) autoFarm = val 
            if val then
                spawn(function()
                    while autoFarm do
                        task.wait(0.3)
                        pcall(function()
                            for _, obj in pairs(workspace:GetDescendants()) do
                                if obj:IsA("Part") and obj.Name:lower():find("brainrot") and player.Character then
                                    local root = player.Character:FindFirstChild("HumanoidRootPart")
                                    if root then
                                        root.CFrame = CFrame.new(obj.Position + Vector3.new(0, 2, 0))
                                        task.wait(0.1)
                                        firetouchinterest(root, obj, 0)
                                        task.wait(0.05)
                                        firetouchinterest(root, obj, 1)
                                    end
                                end
                            end
                        end)
                    end
                end)
            end
        end
    )
    
    -- Auto Money
    createFeatureCard(
        "rbxassetid://6031054644",  -- Icon money
        "Auto Money", 
        "Kumpulin duit otomatis",
        Color3.fromRGB(100, 255, 150),
        function(val) autoMoney = val 
            if val then
                spawn(function()
                    while autoMoney do
                        task.wait(2)
                        pcall(function()
                            local claim = workspace:FindFirstChild("ClaimMoney") or player.PlayerGui:FindFirstChild("ClaimButton")
                            if claim and claim:IsA("TextButton") then
                                claim:Click()
                            end
                        end)
                    end
                end)
            end
        end
    )
    
    -- Anti Tsunami
    createFeatureCard(
        "rbxassetid://6031090990",  -- Icon wave
        "Anti Tsunami", 
        "Otomatis hindari tsunami",
        Color3.fromRGB(100, 150, 255),
        function(val) antiTsunami = val 
            if val then
                spawn(function()
                    while antiTsunami do
                        task.wait(0.5)
                        pcall(function()
                            local tsunami = workspace:FindFirstChild("Tsunami")
                            if tsunami and player.Character then
                                local dist = (player.Character.HumanoidRootPart.Position - tsunami.Position).Magnitude
                                if dist < 50 then
                                    local safe = workspace:FindFirstChild("SafeZone") or workspace:FindFirstChild("Spawn")
                                    if safe then
                                        player.Character.HumanoidRootPart.CFrame = safe.CFrame
                                    end
                                end
                            end
                        end)
                    end
                end)
            end
        end
    )
    
    -- Credit di bagian bawah
    local creditFrame = Instance.new("Frame")
    creditFrame.Size = UDim2.new(1, -10, 0, 60)
    creditFrame.Position = UDim2.new(0, 5, 0, yPos)
    creditFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    creditFrame.BackgroundTransparency = 0.2
    creditFrame.Parent = scroll
    
    local creditCorner = Instance.new("UICorner")
    creditCorner.CornerRadius = UDim.new(0, 16)
    creditCorner.Parent = creditFrame
    
    local creditText = Instance.new("TextLabel")
    creditText.Size = UDim2.new(1, -20, 1, 0)
    creditText.Position = UDim2.new(0, 10, 0, 0)
    creditText.BackgroundTransparency = 1
    creditText.Text = "Created by Alvaro ‚Ä¢ Key: varszzscript"
    creditText.TextColor3 = Color3.fromRGB(150, 150, 150)
    creditText.TextSize = 14
    creditText.Font = Enum.Font.Gotham
    creditText.TextWrapped = true
    creditText.Parent = creditFrame
    
    yPos = yPos + 70
    scroll.CanvasSize = UDim2.new(0, 0, 0, yPos)
    
    -- Notifikasi siap
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "‚úÖ VARSZZHUBüî• SIAP",
        Text = "Semua fitur siap digunakan!",
        Duration = 3
    })
end