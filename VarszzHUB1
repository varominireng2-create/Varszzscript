--[[
    VARSZZHUB V7 - ESCAPE TSUNAMI FOR BRAINROT
    by: Alvaro
    Key: varszzscript
]]

loadstring(game:HttpGet("https://sirius.menu/rayfield"))():CreateWindow({Name="VARSZZHUB V7",KeySystem=true,KeySettings={Key={"varszzscript"}}})

local p=game.Players.LocalPlayer
local f,a=false,false
local r={Secret=true,Celestial=true,Divine=true}

local function cek(n)n=n:lower()
    return(r.Secret and(n:find("secret")or n:find("matteo")))or(r.Celestial and(n:find("celestial")or n:find("job job")))or(r.Divine and(n:find("divine")or n:find("bulbito")))
end

spawn(function()while task.wait()do pcall(function()if p.Character then p.Character.Humanoid.Health=math.huge;p.Character.Humanoid.WalkSpeed=200 end end)end end)

local function farm()
    while f do task.wait(0.1)
        pcall(function()
            for _,o in pairs(workspace:GetDescendants())do
                if(o:IsA("Part")or o:IsA("Model"))and cek(o.Name)and p.Character then
                    local po=o:IsA("Part")and o.Position or o:GetPrimaryPartCFrame().Position
                    p.Character.HumanoidRootPart.CFrame=CFrame.new(po+Vector3.new(0,3,0))
                    task.wait(0.05)
                    if o:IsA("Part")then o.CFrame=CFrame.new(9999,9999,9999)else o:SetPrimaryPartCFrame(CFrame.new(9999,9999,9999))end
                end
            end
        end)
    end
end

local function duit()
    while a do task.wait(2)
        pcall(function()
            local b=workspace:FindFirstChild("ClaimMoney")or p.PlayerGui:FindFirstChild("ClaimButton")
            if b then
                if b:IsA("TextButton")then b:Click()
                elseif b:IsA("Part")and b:FindFirstChildOfClass("ClickDetector")then fireclickdetector(b:FindFirstChildOfClass("ClickDetector"))
                end
            end
        end)
    end
end

local w=Window:CreateTab("MAIN")
w:CreateSection("ðŸŒ¾ AUTO FARM")
w:CreateToggle({Name="Auto Farm",CurrentValue=false,Callback=function(v)f=v;if v then spawn(farm)end end})
w:CreateToggle({Name="Auto Collect Money",CurrentValue=false,Callback=function(v)a=v;if v then spawn(duit)end end})

local rt=Window:CreateTab("RARITY")
rt:CreateSection("âœ¨ FILTER")
for _,v in ipairs({"Secret","Celestial","Divine"})do
    rt:CreateToggle({Name=v.." Rarity",CurrentValue=true,Callback=function(val)r[v]=val end})
end

local it=Window:CreateTab("INFO")
it:CreateSection("ðŸ“Œ INFO")
it:CreateLabel("VARSZZHUB V7")
it:CreateLabel("Key: varszzscript")